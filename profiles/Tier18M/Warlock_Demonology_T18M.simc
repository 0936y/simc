warlock="Warlock_demo_legion"
level=100
race=troll
role=spell
position=back
talents=1202022
spec=demonology

# Executed before combat begins. Accepts non-harmful actions only.

actions.precombat=summon_pet,if=!talent.grimoire_of_supremacy.enabled
actions.precombat+=/summon_doomguard,if=talent.grimoire_of_supremacy.enabled&active_enemies<3
actions.precombat+=/summon_infernal,if=talent.grimoire_of_supremacy.enabled&active_enemies>=3
actions.precombat+=/demonic_empowerment
actions.precombat+=/snapshot_stats

# Executed every time the actor is available.

actions=use_item,name=nithramus_the_allseer
actions+=/service_pet,if=cooldown.summon_doomguard.remains<gcd&soul_shard>=2
actions+=/service_pet,if=cooldown.summon_doomguard.remains>25
actions+=/summon_doomguard,if=talent.grimoire_of_service.enabled&prev.service_felguard&active_enemies<3
actions+=/summon_doomguard,if=talent.grimoire_of_synergy.enabled&active_enemies<3
actions+=/summon_infernal,if=talent.grimoire_of_service.enabled&prev.service_felguard&active_enemies>=3
actions+=/summon_infernal,if=talent.grimoire_of_synergy.enabled&active_enemies>=3
actions+=/call_dreadstalkers,if=!talent.summon_darkglare.enabled
actions+=/hand_of_guldan,if=soul_shard>=4&!talent.summon_darkglare.enabled
actions+=/summon_darkglare,if=prev.hand_of_guldan
actions+=/summon_darkglare,if=prev.call_dreadstalkers
actions+=/summon_darkglare,if=cooldown.call_dreadstalkers.remains>5&soul_shard<3
actions+=/summon_darkglare,if=cooldown.call_dreadstalkers.remains<action.summon_darkglare.cast_time&soul_shard>=3
actions+=/summon_darkglare,if=cooldown.call_dreadstalkers.remains<action.summon_darkglare.cast_time&soul_shard>=1&buff.demonic_calling.up
actions+=/call_dreadstalkers,if=cooldown.summon_darkglare.remains>2
actions+=/call_dreadstalkers,if=prev.summon_darkglare
actions+=/call_dreadstalkers,if=cooldown.summon_darkglare.remains<=action.call_dreadstalkers.cast_time&soul_shard>=3
actions+=/call_dreadstalkers,if=cooldown.summon_darkglare.remains<=action.call_dreadstalkers.cast_time&soul_shard>=1&buff.demonic_calling.up
actions+=/hand_of_guldan,if=soul_shard>=3&prev.call_dreadstalkers
actions+=/hand_of_guldan,if=soul_shard>=5&cooldown.summon_darkglare.remains<=action.hand_of_guldan.cast_time
actions+=/hand_of_guldan,if=soul_shard>=4&cooldown.summon_darkglare.remains>2
actions+=/demonic_empowerment,if=prev.hand_of_guldan
actions+=/demonic_empowerment,if=prev.call_dreadstalkers
actions+=/demonic_empowerment,if=prev.summon_doomguard
actions+=/demonic_empowerment,if=talent.grimoire_of_service.enabled&prev.service_felguard
actions+=/demonic_empowerment,if=prev.summon_darkglare
actions+=/soul_harvest
actions+=/felguard:felstorm
actions+=/doom,cycle_targets=1,if=(!ticking|dot.doom.remains<gcd+4)
actions+=/shadowflame,if=charges=3
actions+=/shadowflame,if=dot.shadowflame.remains<action.shadow_bolt.execute_time
actions+=/life_tap,if=mana.pct<=30
actions+=/demonbolt
actions+=/shadow_bolt

