warlock="T21_Warlock_Affliction"
spec=affliction
level=110
race=human
role=spell
position=back
talents=0100000

# Default consumables
potion=prolonged_power
flask=whispered_pact
food=nightborne_delicacy_platter
augmentation=defiled

# Executed before combat begins. Accepts non-harmful actions only.
actions.precombat=flask
actions.precombat+=/food
actions.precombat+=/augmentation
actions.precombat+=/summon_pet
actions.precombat+=/snapshot_stats
actions.precombat+=/grimoire_of_sacrifice,if=talent.grimoire_of_sacrifice.enabled
actions.precombat+=/potion
actions.precombat+=/haunt
 

# Executed every time the actor is available.
actions=berserking,if=prev_gcd.1.summon_darkglare
actions+=/agony,if=!ticking
actions+=/dark_soul
actions+=/summon_darkglare,if=dot.agony.ticking&dot.corruption.ticking&dot.unstable_affliction_1.ticking&dot.unstable_affliction_2.ticking&dot.unstable_affliction_3.ticking&((dot.unstable_affliction_4.ticking&dot.unstable_affliction_5.ticking)|soul_shard=0)
actions+=/agony,if=remains<=gcd*1.5
actions+=/agony,if=talent.deathbolt.enabled&((dot.agony.remains<=14&cooldown.deathbolt.remains<=gcd&cooldown.summon_darkglare.remains>25)|(dot.agony.remains<=14&cooldown.summon_darkglare.remains<10&cooldown.summon_darkglare.remains>gcd*4))
actions+=/deathbolt,if=cooldown.summon_darkglare.remains>25
actions+=/corruption,if=!ticking
actions+=/siphon_life,if=!ticking
actions+=/unstable_affliction,if=talent.deathbolt.enabled&(cooldown.deathbolt.remains<=gcd*2&cooldown.summon_darkglare.remains>25&cooldown.summon_darkglare.remains<170)
actions+=/corruption,if=talent.deathbolt.enabled&((dot.corruption.remains<10&cooldown.deathbolt.remains<=gcd*4&cooldown.summon_darkglare.remains>25)|(dot.corruption.remains<8&cooldown.summon_darkglare.remains<10&cooldown.summon_darkglare.remains>gcd*4))
actions+=/siphon_life,if=talent.deathbolt.enabled&((dot.siphon_life.remains<10&cooldown.deathbolt.remains<=gcd*4&cooldown.summon_darkglare.remains>25)|(dot.siphon_life.remains<8&cooldown.summon_darkglare.remains<10&cooldown.summon_darkglare.remains>gcd*4))
actions+=/haunt
actions+=/phantom_singularity,if=cooldown.summon_darkglare.remains<12|cooldown.summon_darkglare.remains>45
actions+=/unstable_affliction,if=soul_shard=5|cooldown.summon_darkglare.remains<=gcd*4
actions+=/agony,if=refreshable
actions+=/siphon_life,if=refreshable
actions+=/corruption,if=refreshable
actions+=/unstable_affliction,if=(dot.unstable_affliction_1.ticking+dot.unstable_affliction_2.ticking+dot.unstable_affliction_3.ticking+dot.unstable_affliction_4.ticking+dot.unstable_affliction_5.ticking<=cast_time)&(cooldown.summon_darkglare.remains>35)&(cooldown.deathbolt.remains>=4|!talent.deathbolt.enabled)&soul_shard>=2&!prev_gcd.1.unstable_affliction
actions+=/drain_soul,chain=1,interrupt=1
actions+=/shadow_bolt

