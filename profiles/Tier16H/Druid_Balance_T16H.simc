spec=balance

# This default action priority list is automatically created based on your character.
# It is a attempt to provide you with a action list that is both simple and practicable,
# while resulting in a meaningful and good simulation. It may not result in the absolutely highest possible dps.
# Feel free to edit, adapt and improve it to your own needs.
# SimulationCraft is always looking for updates and improvements to the default action lists.

# Executed before combat begins. Accepts non-harmful actions only.

actions.precombat=flask,type=warm_sun
actions.precombat+=/food,type=mogu_fish_stew
actions.precombat+=/mark_of_the_wild,if=!aura.str_agi_int.up
actions.precombat+=/moonkin_form
# Snapshot raid buffed stats before combat begins and pre-potting is done.
actions.precombat+=/snapshot_stats
actions.precombat+=/jade_serpent_potion

# Executed every time the actor is available.

actions=jade_serpent_potion,if=buff.bloodlust.react|target.time_to_die<=40|buff.celestial_alignment.up
actions+=/blood_fury,if=buff.bloodlust.react|target.time_to_die<=40|buff.celestial_alignment.up
actions+=/berserking,if=buff.bloodlust.react|target.time_to_die<=40|buff.celestial_alignment.up
actions+=/arcane_torrent,if=buff.bloodlust.react|target.time_to_die<=40|buff.celestial_alignment.up
actions+=/run_action_list,name=single_target,if=active_enemies=1
actions+=/run_action_list,name=aoe,if=active_enemies>1
actions+=/force_of_nature,if=talent.force_of_nature.enabled

actions.single_target=celestial_alignment
actions.single_target+=/starsurge,if=charges=3|(eclipse_change>4&eclipse_dir=1)|(eclipse<=-75&eclipse_dir=-1)
actions.single_target+=/moonfire,if=!dot.moonfire.ticking|(dot.moonfire.remains<=8&eclipse_change<=12&eclipse=-100&eclipse_change>=8)|(buff.celestial_alignment.up&dot.moonfire.ticking&dot.sunfire.ticking&dot.sunfire.remains<=6)
actions.single_target+=/sunfire,if=!dot.sunfire.ticking|(eclipse>=0&dot.sunfire.remains<=8)
actions.single_target+=/wrath,if=buff.celestial_alignment.up&buff.solar_empowerment.up&eclipse>0
actions.single_target+=/starfire,if=buff.celestial_alignment.up&buff.lunar_empowerment.up&eclipse<=0
actions.single_target+=/starsurge,if=buff.celestial_alignment.up
actions.single_target+=/starsurge,if=buff.solar_empowerment.down&buff.lunar_empowerment.down&((eclipse>75&eclipse_dir=1)|(eclipse<-75&eclipse_dir=-1))
actions.single_target+=/starfire,if=eclipse<0|(eclipse>0&eclipse_change<2)
actions.single_target+=/wrath

actions.aoe=celestial_alignment
actions.aoe+=/starfall,if=charges=3
actions.aoe+=/moonfire,if=!dot.moonfire.ticking|(dot.moonfire.remains<=8&eclipse_change<=12&eclipse=-100&eclipse_change>=8)|(buff.celestial_alignment.up&dot.moonfire.ticking&dot.sunfire.ticking&dot.sunfire.remains<=6)
actions.aoe+=/sunfire,if=!dot.sunfire.ticking|(eclipse>=0&dot.sunfire.remains<=8)
actions.aoe+=/wrath,if=buff.celestial_alignment.up&buff.solar_empowerment.up&eclipse>0
actions.aoe+=/starfire,if=buff.celestial_alignment.up&buff.lunar_empowerment.up&eclipse<=0
actions.aoe+=/starfall
actions.aoe+=/starfire,if=eclipse<0|(eclipse>0&eclipse_change<2)
actions.aoe+=/wrath

