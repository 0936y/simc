actions+=/moonfire,if=buff.celestial_alignment.up&(!dot.sunfire.ticking|!dot.moonfire.ticking)
actions+=/sunfire,if=buff.solar_eclipse.up&!dot.sunfire.ticking
actions+=/moonfire,if=buff.lunar_eclipse.up&!dot.moonfire.ticking
actions+=/starsurge,if=buff.solar_eclipse.up|buff.lunar_eclipse.up
actions+=/innervate,if=mana.pct<20
actions+=/starsurge,if=(eclipse_dir=1&eclipse<30)|(eclipse_dir=-1&eclipse>-30)
actions+=/starfire,if=eclipse_dir=1
actions+=/wrath,if=eclipse_dir=-1
actions+=/wrath
actions+=/moonfire,moving=1,if=!dot.sunfire.ticking
actions+=/sunfire,moving=1,if=!dot.moonfire.ticking
actions+=/wild_mushroom,moving=1,if=buff.wild_mushroom.stack<5
actions+=/starsurge,moving=1,if=buff.shooting_stars.react
actions+=/moonfire,moving=1
actions+=/sunfire,moving=1
