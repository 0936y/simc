legs=ogreskull_boneplate_greaves,id=115535,bonus_id=566
feet=sabatons_of_fractal_earth,id=113936,bonus_id=566
finger1=phosphorescent_seal,id=113866,bonus_id=566,enchant_id=5327
finger2=spellbound_solium_band_of_sorcerous_strength,id=118300,enchant=gift_of_multistrike
trinket1=forgemasters_insignia,id=113983,bonus_id=566
